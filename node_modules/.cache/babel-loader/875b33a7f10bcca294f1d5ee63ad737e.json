A,UAAA,CAAA,IAAA;AACA,qBAAA,KAAA,GAAA,UAAA,CAAA,KAAA;AACA,qBAAA,OAAA,GAAA,UAAA,CAAA,OAAA;AACA,qBAAA,QAAA,GAAA,UAAA,CAAA,QAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAcA;AACA,IAAA,gBAfA,4BAeA,OAfA,EAeA;AACA,WAAA,cAAA,CAAA,QAAA,GAAA,OAAA;AACA,WAAA,cAAA;AACA,KAlBA;AAmBA;AACA,IAAA,mBApBA,+BAoBA,OApBA,EAoBA;AACA,WAAA,cAAA,CAAA,OAAA,GAAA,OAAA;AACA,WAAA,cAAA;AACA;AAvBA;AAtBA,CAAA","sourcesContent":["<template>\n  <div>\n    <!-- 面包屑导航区域-->\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\n      <el-breadcrumb-item :to=\"{ path: '/home' }\">首页</el-breadcrumb-item>\n      <el-breadcrumb-item>学生管理</el-breadcrumb-item>\n      <el-breadcrumb-item>学生列表</el-breadcrumb-item>\n    </el-breadcrumb>\n    <!-- 卡片视图区域-->\n    <el-card>\n      <!-- 搜索与添加区域-->\n      <el-row :gutter=\"30\">\n        <el-col :span=\"5\">\n          <el-input\n            v-model=\"getStudentBody.mobile\"\n            placeholder=\"请输入学员手机号\"\n            class=\"input-with-select\"\n            clearable\n            @clear=\"getStudentList\"\n          >\n            <el-button\n              slot=\"append\"\n              icon=\"el-icon-search\"\n              @click=\"getStudentList\"\n            ></el-button>\n          </el-input>\n        </el-col>\n      </el-row>\n      <!-- api列表区域-->\n      <el-table border :data=\"studentList\">\n        <el-table-column type=\"index\" width=\"50\" label=\"序号\"></el-table-column>\n        <template v-for=\"(item, index) in tableHead\">\n          <el-table-column\n            v-if=\"item.column_name\"\n            :key=\"index\"\n            :prop=\"item.column_name\"\n            :label=\"item.column_comment\"\n          ></el-table-column>\n        </template>\n      </el-table>\n      <!-- 分页-->\n      <el-pagination\n        :current-page=\"getStudentBody.pageNum\"\n        :page-sizes=\"[10, 20, 50, 100]\"\n        :page-size=\"getStudentBody.pageSize\"\n        layout=\"total, sizes, prev, pager, next, jumper\"\n        :total=\"total\"\n        @size-change=\"handleSizeChange\"\n        @current-change=\"handleCurrentChange\"\n      ></el-pagination>\n    </el-card>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      getStudentBody: {\n        mobile: '13933006634',\n        pageNum: 1,\n        pageSize: 10\n      },\n      // 返回总条数\n      total: 0,\n      // 列表展示数据\n      studentList: {},\n      // table 头名称\n      tableHead: [\n        { column_name: 'course_id', column_comment: '课程ID' },\n        { column_name: 'course_name', column_comment: '课程名称' }\n      ]\n    }\n  },\n  created() {\n    this.getStudentList()\n  },\n  methods: {\n    async getStudentList() {\n      const { data: studentRes } = await this.$api.student.findStudentList(\n        this.getStudentBody\n      )\n      console.log(studentRes)\n      if (studentRes.code !== 200) {\n        return this.$message.error('获取学生课程列表失败！')\n      }\n      this.studentList = studentRes.data\n      this.total = studentRes.total\n      this.pageNum = studentRes.pageNum\n      this.pageSize = studentRes.pageSize\n    },\n    // 监听pagesize改变的事件\n    handleSizeChange(newSize) {\n      this.getStudentBody.pageSize = newSize\n      this.getStudentList()\n    },\n    // 监听 页码值改变的事件\n    handleCurrentChange(newPage) {\n      this.getStudentBody.pageNum = newPage\n      this.getStudentList()\n    }\n  }\n}\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/components/student"}]}