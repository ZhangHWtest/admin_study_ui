l-row>\n      <!-- 用户列表区域-->\n      <el-table :data=\"userList\" border stripe>\n        <el-table-column label=\"ID\" prop=\"id\"></el-table-column>\n        <el-table-column label=\"姓名\" prop=\"name\"></el-table-column>\n        <el-table-column label=\"昵称\" prop=\"nick_name\"></el-table-column>\n        <el-table-column label=\"电话\" prop=\"mobile\"></el-table-column>\n        <el-table-column label=\"邮箱\" prop=\"email\"></el-table-column>\n        <el-table-column label=\"状态\" prop=\"status\"></el-table-column>\n        <el-table-column label=\"部门\" prop=\"dept_name\"></el-table-column>\n        <el-table-column label=\"创建时间\" prop=\"create_time\"></el-table-column>\n        <el-table-column label=\"操作\" width=\"180px\">\n          <template slot-scope>\n            <el-button type=\"primary\" icon=\"el-icon-edit\" size=\"mini\"></el-button>\n            <el-button type=\"danger\" icon=\"el-icon-delete\" size=\"mini\"></el-button>\n            <el-tooltip effect=\"dark\" content=\"分配角色\" placement=\"top\" :enterable=\"false\">\n              <el-button type=\"warning\" icon=\"el-icon-setting\" size=\"mini\"></el-button>\n            </el-tooltip>\n          </template>\n        </el-table-column>\n      </el-table>\n      <!-- 分页-->\n      <el-pagination\n        @size-change=\"handleSizeChange\"\n        @current-change=\"handleCurrentChange\"\n        :current-page=\"queryInfo.pagenum\"\n        :page-sizes=\"[10, 2, 5, 10]\"\n        :page-size=\"queryInfo.pagesize\"\n        layout=\"total, sizes, prev, pager, next, jumper\"\n        :total=\"total\"\n      ></el-pagination>\n    </el-card>\n  </div>\n</template>\n\n<script>\nexport default {\n  created() {\n    this.getUserList()\n  },\n  data() {\n    return {\n      // 获取用户列表的参数对象\n      queryInfo: {\n        query: '',\n        pagenum: 1,\n        pagesize: 10\n      },\n      userList: [],\n      total: 0\n    }\n  },\n  methods: {\n    async getUserList() {\n      const { data: userRes } = await this.$http.get('/user/list', {\n        params: this.queryInfo\n      })\n      if (userRes.code !== 200) {\n        return this.$message.error('获取用户列表失败！')\n      }\n      this.userList = userRes.data.users\n      this.total = userRes.data.total\n    },\n    // 监听pagesize改变的事件\n    handleSizeChange(newSize) {\n      this.queryInfo.pagesize = newSize\n      this.getUserList()\n    },\n    // 监听 页码值改变的事件\n    handleCurrentChange(newPage) {\n      this.queryInfo.pagenum = newPage\n      this.getUserList()\n    }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n</style>\n"]}]}